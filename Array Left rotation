#include <stdio.h>
int main() 
{  
    int n,r;
    
    scanf("%d %d",&n,&r);
    
    int a[n];
    
    for(int i=0;i<n;i++)
        {
            scanf("%d",&a[i]);
    }
    
    int *temp;  
    
    for(int i=0;i<r;i++)
        {
            temp=a+n-1;
            for(int j=n-2;j>=0;j--)
                {
                    (*temp)^=(*(a+j))^=(*(temp))^=(*(a+j));
            }
            *(a+n-1)=*(temp);
    }
    
    for(int i=0;i<n;i++)
        {
            printf("%d ",a[i]);
    }
    
    return 0;
}
